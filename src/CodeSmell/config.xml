<?xml version="1.0"?>
<!DOCTYPE module PUBLIC
          "-//Checkstyle//DTD Checkstyle Configuration 1.3//EN"
          "https://checkstyle.org/dtds/configuration_1_3.dtd">
<module name="Checker">
  <module name="TreeWalker">
          <!-- your standard Checks that come with Checkstyle -->
    <module name="UpperEll"/>
    <module name="MethodLength"/>
          <!-- your Check goes here -->
    <module name="CodeSmell.duplicateCodeCheck">
      <property name="max" value="1"/>
    </module>
	<module name="CodeSmell.SwissArmyKnifeCheck">
      <property name="maxLines" value="50"/>
	  <property name="maxMethods" value="30"/>
    </module>
    <module name="CodeSmell.MethodLimitCheck">
      <property name="max" value="45"/>
    </module>
  </module>
</module>